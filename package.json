{
  "name": "mdx-blog",
  "version": "1.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server": "yarn build && NODE_ENV=production node server.js",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "export": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll",
    "deploy": "gh-pages --dotfiles=true --dist out/"
  },
  "dependencies": {
    "@mdx-js/loader": "2.1.1",
    "@mdx-js/react": "^2.1.1",
    "next": "^12.1.5",
    "parse-numeric-range": "^1.2.0",
    "prism-react-renderer": "^1.3.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "4.3.1",
    "remark-gfm": "^3.0.1",
    "styled-components": "^5.3.5"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "16.11.31",
    "@types/parse-numeric-range": "^0.0.1",
    "@types/react": "18.0.8",
    "@types/react-dom": "^18.0.2",
    "@types/styled-components": "^5.1.4",
    "babel-plugin-import-glob-array": "^0.2.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.5",
    "express": "4.18.0",
    "gh-pages": "^3.1.0",
    "gray-matter": "^4.0.2",
    "mem": "^8.0.0",
    "next-offline": "^5.0.3",
    "prettier": "2.6.2",
    "stringify-object": "^3.3.0",
    "terser": "^4.6.13",
    "typescript": "4.6.3"
  }
}
